                type: 'GET',
                url: 'http://164.164.122.187:80/Identity/Users/GetUserDetails',
                contentType: "application/json; charset=utf-8",
                dataType: 'json',

                success: function (response) {
                    $('#fullname').html(response.firstName_Eng + " " + response.fatherName_Eng + " " + response.grandFatherName_Eng);
                   // console.log(response)
                    var data = {
                        UserId: response.id
                            }
                    $.ajax({
                        type: 'GET',
                        url: 'http://164.164.122.187:80/api/UserInfo/GeUserInfoByUserId',
                        data:data,
                        contentType: "application/json; charset=utf-8",
                        dataType: 'json',
                        success: function (response) {
                            //console.log(response);
                            $('#organization').html(response.organizationName);
                            if (response.locationHierarchy == null) {
                                $('#cdateTime').html('Ethiopia');
                            }
                            else {
                                $('#cdateTime').html(response.locationHierarchy);
                            }
                        },
                        error: function (error) {
                            console.log(error);

                        }
                    });
                },
                error: function (error) {
                    console.log(error);

                }
            });
