        <script>
            function loadNavigation(pgName, gId, pgId, plinkId) {
                var pathName = window.location.pathname;
                var path = pathName.split("/");
                var fPath = path[1];
                var sPath = path[2];
                var tlen = path.length;
                var sPath = path[tlen - 1];
                if (pgName == sPath) {
                    $('.' + gId).addClass('active');
                    $('.' + pgId).addClass('active');
                    $('.' + pgId + ' a:first').addClass('subdrop');
                    $('.' + plinkId).addClass('active');
                }
            }
            function loadNavigationTargeting(pgName, gId, pgId,tarID, plinkId) {
                var pathName = window.location.pathname;
                var path = pathName.split("/");
                var fPath = path[1];
                var sPath = path[2];
                var tlen = path.length;
                var sPath = path[tlen - 1];
                if (pgName == sPath) {
                    $('.' + gId).addClass('active');
                    $('.' + pgId).addClass('active');
                    $('.' + tarID).addClass('active');
                    $('.' + tarID + ' a:first').addClass('subdrop');
                    $('.' + plinkId).addClass('active');
                }
            }

             function loadNavigationTargetingWithGUId(pgName, gId, pgId,tarID, plinkId) {
                var pathName = window.location.pathname;
                var path = pathName.split("/");
                var fPath = path[1];
                var sPath = path[2];
                var tlen = path.length;
                var sPath = path[tlen - 2];
                if (pgName == sPath) {
                    $('.' + gId).addClass('active');
                    $('.' + pgId).addClass('active');
                    $('.' + tarID).addClass('active');
                    $('.' + tarID + ' a:first').addClass('subdrop');
                    $('.' + plinkId).addClass('active');
                }
            }
        </script>
        <script>

            var currentdate = new Date();

                        var weekday = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"];
                        var month = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];

                        var hours = currentdate.getHours();
                        var minutes = currentdate.getMinutes();
                        var ampm = hours >= 12 ? 'PM' : 'AM';
                        hours = hours % 12;
                        hours = hours ? hours : 12; // the hour '0' should be '12'
                        minutes = minutes < 10 ? '0' + minutes : minutes;
                        var strTime = hours + ':' + minutes + ' ' + ampm;


                        var datetime = strTime + ", "
                            + weekday[currentdate.getDay()] + ", "
                            + month[currentdate.getMonth()] + " "
